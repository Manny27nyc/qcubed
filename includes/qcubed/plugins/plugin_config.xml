<?xml version="1.0"?>
<plugins>
	

<plugin>
	<name><![CDATA[QJqSelectMenu]]></name>
	<description><![CDATA[jQuery UI Selectmenu: An ARIA-Accessible Plugin for Styling a Custom HTML Select Element.]]></description>
	<version>0.1</version>
	<platform_version>2.2</platform_version>
	<author name="Oleg Abrosimov" email="olegabr [at] yandex [dot] ru"/>
	<files>
		<file type="js" filename="jquery.ui.selectmenu.js" />
		<file type="css" filename="jquery.ui.selectmenu.css" />
		<file type="control" filename="includes/QJqSelectMenu.class.php" />
		<file type="control" filename="includes/QJqSelectMenuBase.class.php" />
		<file type="control" filename="includes/QJqSelectMenuGen.class.php" />
		<file type="example" filename="example/selectmenu.php" />
		<file type="example" filename="example/selectmenu.tpl.php" />
	</files>
	<includes>
		<include_files classname="QJqSelectMenu" filename="includes/QJqSelectMenu.class.php" />
		<include_files classname="QJqSelectMenuBase" filename="includes/QJqSelectMenuBase.class.php" />
		<include_files classname="QJqSelectMenuGen" filename="includes/QJqSelectMenuGen.class.php" />
	</includes>
	<examples>
		<example filename="example/selectmenu.php" description="QJqSelectMenu: tree view/editor control based on jQuery selectmenu plugin" />
	</examples>
</plugin>

